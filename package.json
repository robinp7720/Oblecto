{
  "name": "oblecto",
  "version": "v0.2.9",
  "description": "Oblecto is a frontend agnostic media butler bringing high performance high quality media streaming to the home ",
  "author": "Robin <robin@mysunland.org>",
  "repository": "robinp7720/Oblecto",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "bin": {
    "oblecto": "./dist/bin/oblecto.js"
  },
  "files": [
    "dist/",
    "res/",
    "Oblecto-Web/dist/",
    "images/"
  ],
  "scripts": {
    "build:web": "cd Oblecto-Web; npm install --unsafe-perms; npm run build; cd ..;",
    "build": "./node_modules/@babel/cli/bin/babel.js src -d dist",
    "prepare": "npm run build && npm run build:web",
    "start": "./node_modules/@babel/node/bin/babel-node.js src",
    "debug": "./node_modules/@babel/node/bin/babel-node.js src --inspect=0.0.0.0",
    "nodemon": "nodemon src --exec './node_modules/@babel/node/bin/babel-node.js src --inspect=0.0.0.0'",
    "test:mocha": "mocha tests/mocha",
    "test:startup": "scripts/test.sh",
    "test": "npm run test:mocha; npm run test:startup"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "fastest-levenshtein": "^1.0.12",
    "fluent-ffmpeg": "^2.1.2",
    "guessit-exec": "^0.0.1",
    "guessit-wrapper": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.30",
    "mkdirp": "^1.0.4",
    "moviedb-promise": "^3.1.8",
    "mysql2": "^2.2.5",
    "neo-blessed": "^0.2.0",
    "node-rsa": "^1.1.1",
    "node-tvdb": "^4.1.0",
    "recursive-readdir": "^2.2.2",
    "restify": "^8.5.1",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^8.0.2",
    "sequelize": "^6.6.2",
    "sharp": "^0.28.1",
    "socket.io": "^4.0.1",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@types/bcrypt": "^3.0.1",
    "@types/restify-errors": "^4.3.3",
    "@types/socket.io": "^2.1.13",
    "eslint": "^7.25.0",
    "eslint-plugin-jsdoc": "^32.3.3",
    "chai": "^4.3.3",
    "expect.js": "^0.3.1",
    "jshint": "^2.12.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7"
  }
}
