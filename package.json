{
  "name": "oblecto",
  "version": "0.1.15",
  "description": "Oblecto is a frontend agnostic media butler bringing high performance high quality media streaming to the home ",
  "author": "Robin <robin@mysunland.org>",
  "repository": "robinp7720/Oblecto",
  "private": false,
  "bin": {
    "oblecto": "./bin/oblecto.js"
  },
  "files": [
    "dist/",
    "bin/",
    "Oblecto-Web/dist/",
    "images/"
  ],
  "scripts": {
    "build:web": "cd Oblecto-Web; npm install --unsafe-perms; npm run build; cd ..;",
    "build": "./node_modules/@babel/cli/bin/babel.js src -d dist",
    "prepare": "npm run build && npm run build:web",
    "start": "./node_modules/@babel/node/bin/babel-node.js  src",
    "debug": "nodemon src --exec 'babel-node src'"
  },
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "async": "^2.6.3",
    "bcrypt": "^2.0.1",
    "epinfer": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "guessit-exec": "0.0.1",
    "guessit-wrapper": "^1.0.1",
    "handbrake-js": "^3.0.8",
    "jimp": "^0.6.8",
    "jsonwebtoken": "^8.5.1",
    "mdns": "^2.4.0",
    "mkdirp": "^0.5.1",
    "moviedb": "^0.2.10",
    "moviedb-promise": "^1.4.2",
    "mysql2": "^1.7.0",
    "node-tvdb": "^3.2.0",
    "node-uuid": "^1.4.8",
    "promise-async": "^0.2.0",
    "recursive-readdir": "^2.2.2",
    "request": "^2.88.0",
    "restify": "^7.7.0",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^6.1.1",
    "sequelize": "^4.44.3",
    "socket.io": "^2.3.0",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "jshint": "^2.10.3",
    "nodemon": "^1.19.4"
  }
}
