{
  "name": "oblecto",
  "version": "v0.2.9",
  "description": "Oblecto is a frontend agnostic media butler bringing high performance high quality media streaming to the home ",
  "author": "Robin <robin@mysunland.org>",
  "repository": "robinp7720/Oblecto",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "bin": {
    "oblecto": "./dist/bin/oblecto.js"
  },
  "files": [
    "dist/",
    "res/",
    "Oblecto-Web/dist/",
    "images/"
  ],
  "type": "commonjs",
  "scripts": {
    "build:web": "cd Oblecto-Web; npm install --unsafe-perms --legacy-peer-deps; npm run build; cd ..;",
    "build": "npx tsc --build; chmod +x ./dist/bin/oblecto.js",
    "prepare": "npm run build",
    "start": "ts-node src/index.js",
    "debug": "node --inspect=0.0.0.0 -r ts-node/register src/index.js",
    "nodemon": "nodemon src --exec 'node --inspect=0.0.0.0 -r ts-node/register src/index.js'",
    "test:mocha": "mocha tests/mocha",
    "test:startup": "scripts/test.sh",
    "test": "npm run test:mocha"
  },
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.13.2",
    "basic-ftp": "^5.0.5",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "fastest-levenshtein": "^1.0.16",
    "fluent-ffmpeg": "^2.1.3",
    "guessit-exec": "^0.0.1",
    "guessit-wrapper": "^1.0.1",
    "jsonwebtoken": "^9.0.3",
    "mime-types": "^3.0.2",
    "mkdirp": "^3.0.1",
    "moviedb-promise": "^4.0.7",
    "mysql2": "^3.16.0",
    "neo-blessed": "^0.2.0",
    "node-rsa": "^1.1.1",
    "node-tvdb": "^4.1.0",
    "primus": "^8.0.9",
    "recursive-readdir": "^2.2.3",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^13.0.0",
    "which": "^6.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "chai": "^6.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^61.5.0",
    "expect.js": "^0.3.1",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
